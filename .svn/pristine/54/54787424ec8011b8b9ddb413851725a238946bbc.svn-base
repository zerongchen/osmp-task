# myclient对应的微服务名
myclient.ribbon.DeploymentContextBasedVipAddresses=serviceapi

# 固定写法，myclient使用的ribbon负载均衡器
myclient.ribbon.NIWSServerListClassName=com.netflix.niws.loadbalancer.DiscoveryEnabledNIWSServerList

# 每分钟更新myclient对应服务的可用地址列表
myclient.ribbon.ServerListRefreshInterval=60000
myclient.ribbon.ReadTimeout=60000
myclient.ribbon.ConnectTimeout=60000
myclient.ribbon.MaxAutoRetries=0
myclient.ribbon.MaxAutoRetriesNextServer=1

hystrix.command.default.execution.timeout.enabled=false

# 控制是否注册自身到eureka中
eureka.registration.enabled=false

# eureka相关配置
eureka.preferSameZone=true
eureka.shouldUseDns=false
eureka.serviceUrl.default=http://localhost:9000/eserver/eureka/

eureka.decoderName=JacksonJson
